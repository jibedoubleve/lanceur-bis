<Application
    x:Class="Lanceur.Ui.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--  WPF-Ui  -->
                <ui:ThemesDictionary Theme="Dark" />
                <ui:ControlsDictionary />
                <!--  LANCEUR  -->
                <ResourceDictionary Source="/Lanceur.Ui.WPF;component/Xaml/Converters.xaml" />
                <ResourceDictionary Source="/Lanceur.Ui.WPF;component/Xaml/Styles.xaml" />
                <ResourceDictionary Source="/Lanceur.Ui.WPF;component/Xaml/Templates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>